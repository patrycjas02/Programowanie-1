#include <iostream>
#include <math.h>
#include <ctime>

using namespace std;

int main (){

    cout<<"Podaj liczbe naturalna z zakresu <1;20>: ";
    int n;
    cin>>n;

    int *A;
    A=new int[n];

    for (int i=0, *pointer=A; i<n; i++){
        *pointer=pow(2,i);
        cout<<*pointer<<" ";
        pointer++;
    }
    cout<<endl;

return 0;
}
