#include <iostream>
#include <math.h>
#include <ctime>
#include <cstdlib>
#include <string>

using namespace std;

string first(string s1, string s2){
    if(s1<s2){
        return s1;
    }
    return s2;
}

int main (){

    string slowo1;
    cin>>slowo1;

    string slowo2;
    cin>>slowo2;

    cout<<first(slowo1,slowo2)<<endl;

return 0;
}
