#define _USE_MATH_DEFINES
#include <iostream>
#include <cmath>
#include <cstring>

using namespace std;

int Fibonacci(int n){

    if((n==0)||(n==1)){
        return n;
    }
    else return Fibonacci(n-1)+Fibonacci(n-2);

}

int main (){

    cout<<"Ktora liczbe Fibonacciego wyswietlic? "<<endl;

    int x;
    cin>>x;

    cout<<Fibonacci(x)<<endl;

return 0;
}
