#include <iostream>
#include <math.h>
#include <ctime>

using namespace std;

int main (){

    int n=20;
    int D[n][n][3];

    srand(time(NULL));

    for (int i=0; i<n; i++){
        for (int j=0; j<n; j++){
            for (int k=0; k<3; k++){
                D[i][j][k]=rand()%256+0;
                cout<<D[i][j][k]<<" ";
            }
            cout<<endl;
        }
        cout<<endl;
    }

        cout<<endl;

return 0;
}
