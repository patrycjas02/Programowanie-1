#include <iostream>
#include <math.h>
#include <ctime>

using namespace std;

int main (){

    int n;
    cin>>n;

    int *A=new int[n];

    srand(time(NULL));

    for (int i=0, *pointer=A; i<n; i++){
        *pointer=rand()%11+0;
        cout<<*pointer<<" ";
        pointer++;
    }

return 0;
}
